{
  "name": "@intoto-dev/bibliotheca",
  "version": "0.0.10",
  "description": "Component library for water data visualisation. By Intoto",
  "engines": {
    "node": "^14.16.0",
    "npm": "^6.14.0"
  },
  "private": true,
  "scripts": {
    "clean": "rimraf packages/**/lib && rimraf packages/**/node_modules && lerna bootstrap",
    "test": "CI=true jest --env=jsdom",
    "prepare": "husky install",
    "start": "start-storybook -p 6006",
    "lint": "eslint packages --max-warnings 0",
    "build-storybook": "build-storybook",
    "prebuild-packages": "npm run clean",
    "build-packages": "lerna exec --parallel -- rollup --config ../../rollup.config.js",
    "prepublish-all": "npm run build-packages",
    "publish-all": "sh ./scripts/publish.sh",
    "postinstall": "lerna bootstrap"
  },
  "keywords": [],
  "author": "Intoto AS",
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@intoto-dev|d3-time-format)",
      "(?!bibliotheca)"
    ]
  },
  "dependencies": {
    "@harelpls/storybook-addon-materialui": "^2.0.0",
    "@material-ui/core": "^4.12.3",
    "@storybook/react": "^6.3.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "ncp": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.0",
    "commitlint": "^13.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-flowtype": "^6.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "lerna": "^4.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "tslib": "^2.3.1"
  }
}
